# over 60min offset

[ "$th" = $((17 - 6)) ] && Thu=59
[ "$th" = $((19 - 6)) ] && Sat=120; [ "$th" = $((20 - 6)) ] && Sat=0
[ "$th" = $((21 - 6)) ] && Sat=120; [ "$th" = $((22 - 6)) ] && Sat=0
[ "$th" = $((27 - 6)) ] && Sun=180; [ "$th" = $((28 - 6)) ] && export {Mon,Tue,Wed,Thu,Fri,Sat}=120 && Sun=0
